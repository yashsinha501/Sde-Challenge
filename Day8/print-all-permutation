void solve(int idx,string s,vector<string> &ans){
    if(idx==s.length()){
        ans.push_back(s);
        return;
    }
    
    for(int i=idx;i<s.length();i++){
        swap(s[i],s[idx]);
        solve(idx+1,s,ans);
        swap(s[i],s[idx]);
    }
    
}

vector<string> findPermutations(string &s) {
    vector<string> ans;
    solve(0,s,ans);
    return ans;
}

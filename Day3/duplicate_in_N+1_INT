#include <bits/stdc++.h> 
int findDuplicate(vector<int> &arr, int n){
	
    int slow=arr[0];
    int fast=arr[0];
    
    do{
        fast=arr[arr[fast]];
        slow=arr[slow];
    }while(fast!=slow);
  fast = arr[0];
  while (slow != fast) {
    slow = arr[slow];
    fast = arr[fast];
  }
    return fast;
    
}

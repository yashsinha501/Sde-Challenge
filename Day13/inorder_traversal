vector<int> getInOrderTraversal(TreeNode *root)
{
    vector<int> ans;
    stack<TreeNode*> st;
    
    while(true){
        if(root!=nullptr){
            st.push(root);
            root=root->left;
        }
        else{
            if(st.empty())break;
            root=st.top();
            ans.push_back(root->data);
            st.pop();
            root=root->right;
        }
    }
    return ans;
}

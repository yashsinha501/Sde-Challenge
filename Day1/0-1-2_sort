#include <bits/stdc++.h> 
void sort012(int *a, int n)
{
        int mid=0;
        int low=0;
        int high=n-1;
        
        while(mid<=high)
        {
            if(a[mid]==0)
            {
                
                int temp=a[low];
                a[low]=a[mid];
                a[mid]=temp;
                mid++;
                low++;
            }
            else if(a[mid]==1)
            {
                mid++;
            }
            else
            {
                int temp=a[mid];
                a[mid]=a[high];
                a[high]=temp;
                high--;
            }
        }
    }

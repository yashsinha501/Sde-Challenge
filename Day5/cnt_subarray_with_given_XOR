#include <bits/stdc++.h> 
int subarraysXor(vector<int> &arr, int x)
{
    unordered_map<int,int> mp;
    int cpx=0;
    int c=0;
    for(int i=0;i<arr.size();i++){
        cpx^=arr[i];
        if(cpx==x)c++;
        int h=cpx^x;
        if(mp.find(h)!=mp.end()){
            c=c+mp[h];
        }
        mp[cpx]++;
    }
    return c;
}
